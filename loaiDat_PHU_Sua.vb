                End Try

                '==============SO SÁNH <= 1 SỐ=========D1E2f1b	Độ sâu tầng loang lổ (cm)   0 < và <= 100
                Dim D1E2f1b As ULong = 0
                Try
                    If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f1b"), TextEdit).Text <> "" Then
                        If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f1b"), TextEdit).Text <= 100 _
                                And CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f1b"), TextEdit).Text > 0 Then
                            D1E2f1b = 1
                        ElseIf CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f1b"), TextEdit).Text > 100 Then
                            D1E2f1b = 0              'Có phải ko???
                        End If
                    End If
                Catch ex As Exception
                End Try
                '================TỔNG HỢP==========
                Dim D1E2f1 As Decimal = D1E2f1a * D1E2f1b
                '===========================
                Stop_XacDinh_F = KetLuanF(D1E2f1, LoaiDatPhu, thesender)
