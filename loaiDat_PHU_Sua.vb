                        ElseIf CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f3b"), TextEdit).Text > 100 Then
                            D1E2f3b = 0              'Có phải ko???
                        End If
                    End If
                Catch ex As Exception
                End Try
                '============== ĐÚNG/SAI =========D1E2f3c	Lớp tích lũy kết von > 15% (thể tích)
                Dim D1E2f3c As ULong = 0
                Try
                    If CType(FindControl(F_CbxCollection, "Cbx" + "D1E2f3c"), LookUpEdit).EditValue = "Đúng" Then
