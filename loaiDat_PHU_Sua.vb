                            D1E2f3d = 0          'Có phải ko???
                        End If
                    End If
                Catch ex As Exception
                End Try

                '==============SO SÁNH <= 1 SỐ=========D1E2f3e	Độ sâu xuất hiện kết von (cm)	25 <= và <= 100
                Dim D1E2f3e As ULong = 0
                Try
                    If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f3e"), TextEdit).Text <> "" Then
                        If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f3e"), TextEdit).Text <= 100 _
                                And CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f3e"), TextEdit).Text >= 25 Then
                            D1E2f3e = 1
                        ElseIf CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f3e"), TextEdit).Text > 100 Then
                            D1E2f3e = 0              'Có phải ko???
                        End If
                    End If
                Catch ex As Exception
                End Try
                '================TỔNG HỢP==========
                Dim D1E2f3 As Decimal = D1E2f3a * D1E2f3b * D1E2f3c * D1E2f3d * D1E2f3e
                '===========================
                Stop_XacDinh_F = KetLuanF(D1E2f3, LoaiDatPhu, thesender)
                If Stop_XacDinh_F = True Then
                    Exit For
                Else
                End If
            ElseIf LoaiDatPhu = "Đất cát biển lẫn vỏ sò- hến- điệp" Then
                '========//////////////////////Xóa \\\\\\\\\\\\\\\\\==========D1E2f4	ĐẤT CÁT BIỂN LẪN VỎ SÒ- HẾN- ĐIỆP       Đất cát biển lẫn vỏ sò- hến- điệp
                '============== ĐÚNG/SAI =========D1E2f4a	Lẫn vỏ sò, hến, điệp  ≥ 10% (thể tích) 
                Dim D1E2f4a As ULong = 0
                Try
                    If CType(FindControl(F_CbxCollection, "Cbx" + "D1E2f4a"), LookUpEdit).EditValue = "Đúng" Then
                        D1E2f4a = 1
                    ElseIf CType(FindControl(F_CbxCollection, "Cbx" + "D1E2f4a"), LookUpEdit).EditValue = "Sai" Then
                        D1E2f4a = 0           'Có phải ko???
                    End If
                Catch ex As Exception
                End Try
                '==============SO SÁNH >= 1 SỐ=========D1E2f4b	Độ dày lớp lẫn vỏ sò, hến, điệp (cm)	> 15
                Dim D1E2f4b As ULong = 0
                Try
                    If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4b"), TextEdit).Text <> "" Then
                        If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4b"), TextEdit).Text >= 15 Then
                            D1E2f4b = 1
                        ElseIf CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4b"), TextEdit).Text < 15 _
                                  And CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4b"), TextEdit).Text > 0 Then
                            D1E2f4b = 0          'Có phải ko???
                        End If
                    End If
                Catch ex As Exception
                End Try

                '==============SO SÁNH <= 1 SỐ=========D1E2f4c	Độ sâu xuất hiện lớp lẫn vỏ sò, hến, điệp (cm)	0 < và <= 100
                Dim D1E2f4c As ULong = 0
                Try
                    If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4c"), TextEdit).Text <> "" Then
                        If CType(FindControl(F_TXTCollection, "Tbx" + "D1E2f4c"), TextEdit).Text <= 100 _
