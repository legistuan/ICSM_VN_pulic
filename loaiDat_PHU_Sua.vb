#Region "LoaiDat phu SỬA"
    Private Function XacDinh_fLoaiDatPhuFromListBoxF(ByVal alistbox As ListBoxControl, thesender As Object) As Boolean
        XacDinh_fLoaiDatPhuFromListBoxF = False
        Select_All = 1      'Không cho chạy listbx Selected indexchanged.
        ' ListBoxControlF.SelectAll()
        Select_All = 0     'Kết thúc Select All
        For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
            If ctrl.GetType <> GetType(LabelControl) Then
                ctrl.BackColor = Color.Wheat
            End If
        Next
        ChoPhepChayListboxSelectedIndexChanged = 0
        Dim F_ControlCollection As New List(Of Control)
        For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
            If ctrl.GetType <> GetType(LabelControl) Then
                F_ControlCollection.Add(ctrl)
            End If
        Next
        Dim F_CbxCollection As New List(Of Control)
        Dim F_TXTCollection As New List(Of Control)
        For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
            If ctrl.GetType = GetType(LookUpEdit) Then
                F_CbxCollection.Add(ctrl)
            ElseIf ctrl.GetType = GetType(TextEdit) Then
                F_TXTCollection.Add(ctrl)

            End If
        Next
        Dim Stop_XacDinh_F As Boolean = False         'Để dừng vòng lặp xác định LOẠI đất khi đã xác định được là LOẠI đất nào (=true thì stop)

        'Không cần vòng lặp for này, vì sẽ chạy đầy đủ các Loại đất từ đầu tới cuối theo điều kiện NHÓM ĐẤT
        For i As Int16 = 0 To ListBoxControlF.ItemCount - 1
            If Stop_XacDinh_F = True Then
                Exit For
            End If
            Dim LoaiDatPhu As String = ListBoxControlF.Items(i).ToString

            '===========================
            If LoaiDatPhu = "Đất cồn cát trắng, vàng" Then
                '========//////////////////////Xóa \\\\\\\\\\\\\\\\\===========D1E1f1	Đất cồn cát trắng, vàng
                '============== ĐÚNG/SAI =====KẾT HỢP========D1E1f1	Đất cồn cát có màu Munsell với Hue 10YR hoặc vàng hơn, Value > 6
                Dim D1E1f1 As ULong = 0
                Try
                    If CType(FindControl(F_CbxCollection, "Cbx" + "D1E1f1"), LookUpEdit).EditValue = "Đúng" Then
                        D1E1f1 = 1
                        MessageBox.Show("Khẳng định là ""Đất cồn cát trắng, vàng""")
                        Exit Function
                    ElseIf CType(FindControl(F_CbxCollection, "Cbx" + "D1E1f1"), LookUpEdit).EditValue = "Sai" Then
                        D1E1f1 = 0           'Có phải ko???
                        alistbox.SetSelected(ListBoxControlF.Items.IndexOf("Đất cồn cát trắng, vàng"), False)
                    End If
                Catch ex As Exception
                End Try
                '===========================
                Stop_XacDinh_F = KetLuanF(D1E1f1, LoaiDatPhu, thesender)
                If Stop_XacDinh_F = True Then
                    Exit For
                Else
                End If
            ElseIf LoaiDatPhu = "Đất cồn cát đỏ" Then
                '========//////////////////////Xóa \\\\\\\\\\\\\\\\\===========D1E1f2	Đất cồn cát đỏ
                '============== ĐÚNG/SAI =====KẾT HỢP========D1E1f2	Đất cồn cát có Hue 10R hoặc đỏ hơn, Value ≥ 5 và Chroma ≥ 6
                Dim D1E1f2 As ULong = 0
                Try
                    If CType(FindControl(F_CbxCollection, "Cbx" + "D1E1f2"), LookUpEdit).EditValue = "Đúng" Then
                        D1E1f2 = 1
                        MessageBox.Show("Khẳng định là ""Đất cồn cát đỏ""")
                        Exit Function
                    ElseIf CType(FindControl(F_CbxCollection, "Cbx" + "D1E1f2"), LookUpEdit).EditValue = "Sai" Then
                        D1E1f2 = 0           'Có phải ko???
                        alistbox.SetSelected(ListBoxControlF.Items.IndexOf("Đất cồn cát đỏ"), False)
                    End If
                Catch ex As Exception
                End Try
                '===========================
                Stop_XacDinh_F = KetLuanF(D1E1f2, LoaiDatPhu, thesender)
                If Stop_XacDinh_F = True Then
                    Exit For
                Else
                End If
            ElseIf LoaiDatPhu = "Đất cồn cát điển hình" Then
                '========//////////////////////Xóa \\\\\\\\\\\\\\\\\=========== D1E1f3	Đất cồn cát điển hình
                '============== ĐÚNG/SAI =========KẾT HỢP=====D1E1f3	Không phải I.1.1 và I.1.2

                Dim D1E1f3 As ULong = 0
                Try
                    If CType(FindControl(F_CbxCollection, "Cbx" + "D1E1f3"), LookUpEdit).EditValue = "Đúng" Then
