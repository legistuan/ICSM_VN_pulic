                    If j = 8 Then
                        Dim a = 1
                    End If
                Next
            Next
            For i As Integer = 0 To kq1.Length - 1
                kq(i) = kq1(i) + kq2(i) + kq3(i) + kq4(i) + kq5(i) + kq6(i)
            Next

            disableALL_ChungDat_Controls()
            If kq(0) = 0 Then     'txt1 = 0 Then
                txtCD_1TPCG.Enabled = True
                Exit Sub
            End If
            If kq(1) = 0 Then     'txt2 = 0 Then
                txtCD_2DungTrong.Enabled = True
                Exit Sub
            End If
            If kq(2) = 0 Then     'txt3 = 0 Then
                txtCD_3pKHCl.Enabled = True
                Exit Sub
            End If
            If kq(3) = 0 Then     'txt4 = 0 Then
                txtCD_4OM.Enabled = True
                Exit Sub
            End If
            If kq(4) = 0 Then     'txt5 = 0 Then
                txtCD_5CEC.Enabled = True
                Exit Sub
            End If
            If kq(5) = 0 Then     'txt6 = 0 Then
                txtCD_6DoDay.Enabled = True
                Exit Sub
            End If
            If kq(6) = 0 Then     'txt7 = 0 Then
                txtCD_7SoiSan.Enabled = True
                Exit Sub
            End If
            If kq(7) = 0 Then     'txt8 = 0 Then
                txtCD_8BS.Enabled = True
                Exit Sub
            End If
            If kq(8) = 0 Then     'txt9 = 0 Then
                Cbx_KhoangVat.Enabled = True
                Exit Sub
            End If

        End If

    End Sub
    Function XacDinhCoTuKhoa(Input As String, txt1 As Integer, txt2 As Integer, txt3 As Integer, txt4 As Integer, txt5 As Integer, txt6 As Integer, txt7 As Integer, txt8 As Integer, txt9 As Integer) As Array
        Dim tpcg As String = "cơ giới"
        Dim chat As String = "Chặt"
        Dim dungtrong As String = "Dung trọng"
        Dim chua As String = "chua"
        Dim mun As String = "mùn"
        Dim OM As String = "OM"
        Dim cec As String = "CEC"
        Dim doday As String = "Độ dày"
        Dim dalan As String = "đá lẫn"
        Dim bs As String = "BS"
        Dim bazo As String = "bazơ"
        Dim khoangsan As String = "Khoáng sản"
        If Input.ToUpper.Contains(tpcg.ToUpper) Then
            txt1 = txt1 + 1
        End If
        If Input.ToUpper.Contains(chat.ToUpper) Then
            txt2 = txt2 + 1
        End If
        If Input.ToUpper.Contains(chua.ToUpper) And Input.ToUpper.Contains(dungtrong.ToUpper) Then
            txt3 = txt3 + 1
        End If
        If Input.ToUpper.Contains(mun.ToUpper) Then
            txt4 = txt4 + 1
        End If
        If Input.ToUpper.Contains(cec.ToUpper) And Input.ToUpper.Contains(OM.ToUpper) Then
            txt5 = txt5 + 1
        End If
        If Input.ToUpper.Contains(doday.ToUpper) Then
            txt6 = txt6 + 1
        End If
        If Input.ToUpper.Contains(dalan.ToUpper) Then
            txt7 = txt7 + 1
        End If
        If Input.ToUpper.Contains(bs.ToUpper) Then
            txt8 = txt8 + 1
        End If
        If Input.ToUpper.Contains(khoangsan.ToUpper) Then
            txt9 = txt9 + 1
        End If
        Return {txt1, txt2, txt3, txt4, txt5, txt6, txt7, txt8, txt9}
    End Function
