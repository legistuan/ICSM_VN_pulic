    Private Sub btn_ChungDat_Click(sender As Object, e As EventArgs) Handles btn_ChungDat.Click
        If ListBoxControlF.SelectedItems.Count < 1 Then
            Exit Sub
        End If

        Dim ChungDatName As String = ListBoxControlF.SelectedItem.ToString + ", "
        Try
            Dim tpcgVal As Double = CType(txtCD_1TPCG.Text, Double)
            If tpcgVal >= 0 And tpcgVal <= 5 Then
                ChungDatName = ChungDatName + "Cát nhẹ (cát rời)"
            ElseIf tpcgVal >= 5 And tpcgVal < 10 Then
                ChungDatName = ChungDatName + "Cát trung bình"
            ElseIf tpcgVal >= 10 And tpcgVal < 20 Then
                ChungDatName = ChungDatName + "Cát nặng (cát pha)"
            ElseIf tpcgVal >= 20 And tpcgVal < 30 Then
                ChungDatName = ChungDatName + "Thịt nhẹ"
            ElseIf tpcgVal >= 30 And tpcgVal < 40 Then
                ChungDatName = ChungDatName + "Thịt trung bình"
            ElseIf tpcgVal >= 40 And tpcgVal < 50 Then
                ChungDatName = ChungDatName + "Thịt nặng"
            ElseIf tpcgVal >= 50 And tpcgVal < 65 Then
                ChungDatName = ChungDatName + "Sét nhẹ"
            ElseIf tpcgVal >= 65 And tpcgVal < 80 Then
                ChungDatName = ChungDatName + "Sét trung bình"
            ElseIf tpcgVal > 80 Then
                ChungDatName = ChungDatName + "Sét nặng"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim DungTrongVal As Double = CType(txtCD_2DungTrong.Text, Double)
            If DungTrongVal > 1.6 Then
                ChungDatName = ChungDatName + "Chặt cứng"
            ElseIf DungTrongVal >= 1.4 And DungTrongVal <= 1.6 Then
                ChungDatName = ChungDatName + "Rất chặt"
            ElseIf DungTrongVal >= 1.1 And DungTrongVal < 1.4 Then
                ChungDatName = ChungDatName + "Chặt"
            ElseIf DungTrongVal >= 1.0 And DungTrongVal < 1.1 Then
                ChungDatName = ChungDatName + "Hơi xốp"
            ElseIf DungTrongVal < 1 Then
                ChungDatName = ChungDatName + "Rất xốp"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim DoChuaVal As Double = CType(txtCD_3pKHCl.Text, Double)
            If DoChuaVal < 4.5 Then
                ChungDatName = ChungDatName + "Rất chua"
            ElseIf DoChuaVal >= 4.5 And DoChuaVal <= 5.5 Then
                ChungDatName = ChungDatName + "Ít chua"
            ElseIf DoChuaVal >= 5.5 And DoChuaVal < 6.5 Then
                ChungDatName = ChungDatName + "Gần trung tính"
            ElseIf DoChuaVal >= 6.5 And DoChuaVal < 7.5 Then
                ChungDatName = ChungDatName + "Trung tính"
            ElseIf DoChuaVal >= 7.5 Then
                ChungDatName = ChungDatName + "Kiềm"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim OMVal As Double = CType(txtCD_4OM.Text, Double)
            If OMVal < 1 Then
                ChungDatName = ChungDatName + "Rất nghèo"
            ElseIf OMVal >= 1 And OMVal <= 2 Then
                ChungDatName = ChungDatName + "Nghèo"
            ElseIf OMVal >= 2 And OMVal < 3 Then
                ChungDatName = ChungDatName + "Trung bình"
            ElseIf OMVal >= 3 And OMVal < 5 Then
                ChungDatName = ChungDatName + "Khá"
            ElseIf OMVal >= 5 Then
                ChungDatName = ChungDatName + "Giầu"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim CECval As Double = CType(txtCD_5CEC.Text, Double)
            If CECval < 8 Then
                ChungDatName = ChungDatName + "Rất thấp"
            ElseIf CECval >= 8 And CECval <= 16 Then
                ChungDatName = ChungDatName + "Thấp"
            ElseIf CECval >= 16 And CECval < 24 Then
                ChungDatName = ChungDatName + "Trung bình"
            ElseIf CECval >= 24 And CECval < 40 Then
                ChungDatName = ChungDatName + "Cao"
            ElseIf CECval >= 40 Then
                ChungDatName = ChungDatName + "Rất cao"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim DoDayVal As Double = CType(txtCD_5CEC.Text, Double)
            If DoDayVal < 30 Then
                ChungDatName = ChungDatName + "Mỏng"
            ElseIf DoDayVal >= 30 And DoDayVal <= 50 Then
                ChungDatName = ChungDatName + "Hơi mỏng"
            ElseIf DoDayVal >= 50 And DoDayVal < 70 Then
                ChungDatName = ChungDatName + "Trung bình"
            ElseIf DoDayVal >= 70 And DoDayVal < 10 Then
                ChungDatName = ChungDatName + "Dầy"
            ElseIf DoDayVal >= 100 Then
                ChungDatName = ChungDatName + "Rất dầy"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim SoisanVal As Double = CType(txtCD_7SoiSan.Text, Double)
            If SoisanVal >= 70 Then
                ChungDatName = ChungDatName + "Đá lẫn (Abundant)"
            ElseIf SoisanVal > 50 And SoisanVal < 70 Then
                ChungDatName = ChungDatName + "Rất nhiều (Many)"
            ElseIf SoisanVal > 25 And SoisanVal < 50 Then
                ChungDatName = ChungDatName + "Nhiều (Common)"
            ElseIf SoisanVal >= 10 And SoisanVal < 25 Then
                ChungDatName = ChungDatName + "Ít (Few)"
            ElseIf SoisanVal < 10 Then
                ChungDatName = ChungDatName + "Rất ít (Very few)"
            End If
        Catch ex As Exception

        End Try

        Try
            Dim BSval As Double = CType(txtCD_8BS.Text, Double)
            If BSval >= 80 Then
                ChungDatName = ChungDatName + "Rất cao"
            ElseIf BSval > 50 And BSval < 80 Then
                ChungDatName = ChungDatName + "Cao"
            ElseIf BSval > 30 And BSval < 50 Then
                ChungDatName = ChungDatName + "Trung bình"
            ElseIf BSval > 10 And BSval < 30 Then
                ChungDatName = ChungDatName + "Thấp"
            ElseIf BSval <= 10 Then
                ChungDatName = ChungDatName + "Rất thấp"
            End If
        Catch ex As Exception

        End Try
        MessageBox.Show(ChungDatName)
    End Sub

