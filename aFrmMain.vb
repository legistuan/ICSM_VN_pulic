                    End If
                Next
            Next

            For i As Int16 = 0 To dt_PhuLoai.Rows.Count - 1
                Dim thamsoPhuLoai As String = dt_PhuLoai.Rows(i)("ThamSo")
                For j As Integer = 0 To kq6.Length - 1
