        Select_All = 1      'Không cho chạy listbx Selected indexchanged.
        ' ListBoxControlF.SelectAll()
        Select_All = 0     'Kết thúc Select All
        'For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
        '    If ctrl.GetType <> GetType(LabelControl) Then
        '        ctrl.BackColor = Color.Wheat
        '    End If
        'Next
        ChoPhepChayListboxSelectedIndexChanged = 0
        Dim F_ControlCollection As New List(Of Control)
        For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
            If ctrl.GetType <> GetType(LabelControl) Then
                F_ControlCollection.Add(ctrl)
            End If
        Next
        Dim F_CbxCollection As New List(Of Control)
        Dim F_TXTCollection As New List(Of Control)
        For Each ctrl As Control In XtraScrollable_LoaiDatPhu.Controls
            If ctrl.GetType = GetType(LookUpEdit) Then
                F_CbxCollection.Add(ctrl)
            ElseIf ctrl.GetType = GetType(TextEdit) Then
                F_TXTCollection.Add(ctrl)

            End If
        Next
        Dim Stop_XacDinh_F As Boolean = False         'Để dừng vòng lặp xác định LOẠI đất khi đã xác định được là LOẠI đất nào (=true thì stop)

        'Không cần vòng lặp for này, vì sẽ chạy đầy đủ các Loại đất từ đầu tới cuối theo điều kiện NHÓM ĐẤT
        For i As Int16 = 0 To ListBoxControlF.ItemCount - 1
            If Stop_XacDinh_F = True Then
                Exit For
            End If
            Dim LoaiDatPhu As String = ListBoxControlF.Items(i).ToString
            '===========================ĐOẠN NÀY NẾU TÁCH RA CÁC IF THEN ELSE DỰA TRÊN NHÓM ĐẤT HAY LOẠI ĐẬT THÌ TỐC ĐỘ XỬ LÝ SẼ NHANH HƠN RẤT NHIỀU
