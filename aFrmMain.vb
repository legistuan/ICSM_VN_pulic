    Function FileName_from_Export2Excel_Dialog() As String
        Dim f As New SaveFileDialog
        f.Title = "Xuất bảng dữ liệu dưới dạng Excel file"
        f.Filter = "file excel |*.xls;*.xlsx"

        Dim TargetFile As String = ""

        If f.ShowDialog() = DialogResult.OK Then
            TargetFile = f.FileName
        End If
        Return TargetFile
