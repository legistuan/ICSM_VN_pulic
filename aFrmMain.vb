    Private Shared Function FindControl_ContainsName(ByVal root As Control, ByVal target As String) As Control
        If root.Name.Contains(target) Then
            Return root
        End If
        For i As Integer = 0 To root.Controls.Count - 1
            If root.Controls(i).Name.Contains(target) Then
                Return root.Controls(i)
            End If
        Next
        For i As Integer = 0 To root.Controls.Count - 1
            Dim result As Control
            For k As Integer = 0 To root.Controls(i).Controls.Count - 1
                result = FindControl(root.Controls(i).Controls(k), target)
                If result IsNot Nothing Then
                    Return result
                End If
            Next
        Next
        Return Nothing
    End Function
